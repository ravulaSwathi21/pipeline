pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
             echo 'building the poject..' 
            }
        }
        stage('Test') { 
            steps {
               echo 'testing the project....'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'deploying the project.....'
            }
        }
        stage('Release'){
          steps {
            echo 'releasing the project'
          }
        }
         }

